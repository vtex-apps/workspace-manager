{
  "memory": 256,
  "ttl": 10,
  "timeout": 5,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "workspaces": {
      "path": "/_v/workspaces/:name/:isProduction",
      "public": true
    },
    "wsdelete": {
      "path": "/_v/workspaces/delete/:name",
      "public": true
    },
    "wspromote": {
      "path": "/_v/workspaces/promote/:workspace",
      "public": true
    }
  }
}
